{
  "name": "psnjs",
  "version": "1.0.0",
  "description": "",
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "exports": {
    ".": {
      "require": "./dist/lib.js",
      "import": "./dist/esm/lib.mjs",
      "types": "./dist/lib.d.ts"
    },
    "./cli": "./dist/cli.js"
  },
  "bin": {
    "psnjs": "dist/cli.js"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json && node scripts/rename-esm.js",
    "start": "node dist/index.js",
    "dev": "npx ts-node src/index.ts",
    "listen": "npx ts-node src/index-listener.ts",
    "osc": "OSC_ENABLE_TCP=1 npx ts-node src/index.ts",
    "test:osc": "npx ts-node src/osc_integration_test.ts",
    "mcast:listen": "node src/mcast-test.js",
    "mcast:send": "node src/mcast-send.js",
    "send:sim": "npx ts-node src/psnTx.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "files": [
    "dist",
    "README.md",
    "DEVELOPERS.md"
  ],
  "dependencies": {
    "cap": "^0.2.1",
    "events": "^3.3.0",
    "arg": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
